{
  "name": "mock-generator-graphql",
  "version": "1.0.0",
  "description": "A code generator plugin for creating stubs of an API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && yarn graphql-codegen",
    "build:watch": "nodemon --exec yarn start",
    "graphql-codegen": "graphql-codegen",
    "wiremock": "wiremock --root-dir ./mock"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramanujprasad/mock-generator-graphql.git"
  },
  "keywords": [
    "graphql",
    "codegen",
    "wiremock",
    "node"
  ],
  "author": "Ramanuj Prasad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ramanujprasad/mock-generator-graphql/issues"
  },
  "homepage": "https://github.com/ramanujprasad/mock-generator-graphql#readme",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "fs-extra": "^9.0.1",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@types/fs-extra": "^9.0.6",
    "@types/node-fetch": "^2.5.7",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "wiremock-standalone": "^2.26.3-1"
  }
}
